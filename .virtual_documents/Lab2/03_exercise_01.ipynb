def NhapDuLieu():
    """
    Input: 
        Ban phim = 1 5 6
    Output:
        (a, b, c) --> (1, 5, 6)
    """
    """ CÁC BẠN LÀM BÀI Ở ĐÂY """    
    # ...đã làm
    while True:
        try:
            a, b, c = map(float, input("Moi ban nhap he so a, b, c: ").split())
            return a, b, c
        except ValueError:
            print("Ban nhap sai! Moi ban nhap lai!")
# NhapDuLieu



# Kiểm tra hàm
a, b, c = NhapDuLieu()


def GiaiPhuongTrinhBac2(a, b, c):
    """
    Input: a, b, c
    Output: 
    + flag = -1 (VSN), 0 (VN), k (k nghiem)
    + () --> flag = -1, 0
    + (x) --> flag = 1
    + (x1, x2) --> flag = 2
    """
    import math
    flag = None
    x = ()
    
    """ CÁC BẠN LÀM BÀI Ở ĐÂY """
    # ...đã làm
    if a == 0:
        if b == 0:
            if c == 0:
                flag = -1
            else:
                flag = 0
        else:
            flag = 1
            x = (-c/b)
    else:
        delta = b**2 - 4*a*c
        if delta < 0:
            flag = 0
        elif delta == 0:
            flag = 1
            x0 = -b/ (2*a)
            x = (x0)
        else:
            flag = 2
            x1 = (-b + math.sqrt(delta)) / (2*a)
            x2 = (-b - math.sqrt(delta)) / (2*a)
            x = (x1, x2)
    
    return flag, x
# GiaiPhuongTrinhBac2


print("a = 0")
flag, x = GiaiPhuongTrinhBac2(0, 0, 0)
print(f'So nghiem: {flag}, nghiem = {x}')
